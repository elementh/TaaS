{
  "LOG_GROUP_NAME": "Thankifi",
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIPHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*:/api/*",
        "Period": "10s",
        "Limit": 50
      }
    ]
  },
  "Metrics": {
    "Enabled": true
  },
  "AllowedHosts": "*"
}
